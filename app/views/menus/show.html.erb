<div class="page-header">
    <h1>Menu Item <%=@item.id %></h1>
</div>

<div class="row">
    <div class="col-md-12">
        <dl class="dl-horizontal">
            <dt>Menu Item</dt>
            <dd><%= @item.name %></dd>
            
            <dt>Image</dt>
            <dd> <img src="<%= @item.photo %>", width="25%"> </dd>
            
            <dt>Description</dt>
            <dd><%=@item.description %></dd>
            
            <dt>Price</dt>
            <dd><%=@item.price %></dd>
            
            <button type="button" class="btn btn-primary"><a href="/menus">Back</a></button>
            <button type="button" class="btn btn-warning"><a href="/menus/<%= @item.id %>/edit">Edit</a></button>
            <button type="button" class="btn btn-danger"><a href="/menus/<%= @item.id %>" rel="nofollow">Delete</a></a></button>
            
        </dl>
    </div>
    
    
</div>